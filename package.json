{
  "name": "shapeshifter",
  "version": "5.1.0",
  "description": "Generate relational schemas, React prop types, Flow type aliases, or TypeScript interfaces from JSON, JS, or GraphQL schematic files.",
  "keywords": [
    "generate",
    "schema",
    "schematic",
    "react",
    "prop",
    "flow",
    "typescript",
    "type",
    "alias",
    "definitions",
    "interface",
    "struct",
    "shape",
    "shift",
    "shifter",
    "transpile",
    "entity",
    "graphql"
  ],
  "main": "./lib/index.js",
  "module": "./esm/index.js",
  "bin": "./bin/shapeshifter.js",
  "scripts": {
    "babel": "yarn run babel:cjs && yarn run babel:esm",
    "babel:cjs": "beemo babel ./src --out-dir ./lib --copy-files --cjs",
    "babel:esm": "beemo babel ./src --out-dir ./esm --copy-files --esm",
    "check": "type-check && tsc --noEmit --allowJs ./tests/expected/gql/typescript/* ./tests/expected/node/typescript/*",
    "coverage": "yarn run jest --coverage",
    "eslint": "beemo eslint --color --report-unused-disable-directives ./src ./tests",
    "jest": "beemo jest --colors --logHeapUsage",
    "posttest": "yarn run flow --silent",
    "pretest": "yarn run eslint --silent",
    "preversion": "yarn test",
    "postversion": "yarn run babel",
    "test": "yarn run jest --silent",
    "test:webpack": "yarn run babel && webpack --config webpack.config.js",
    "test:webpack:dev": "yarn run babel && webpack-dev-server",
    "flow": "beemo flow check",
    "prettier": "beemo prettier --write ./README.md ./{src,tests}/**/*.{js,json,md}"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/milesj/shapeshifter.git"
  },
  "author": {
    "name": "Miles Johnson",
    "url": "http://milesj.me"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/milesj/shapeshifter/issues"
  },
  "homepage": "https://github.com/milesj/shapeshifter#readme",
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "graphql": "^0.12.3",
    "optimal": "^0.14.0",
    "vorpal": "^1.12.0"
  },
  "devDependencies": {
    "@milesj/build-tool-config": "^0.59.2",
    "babel-loader": "^7.1.2",
    "prop-types": "^15.6.0",
    "typescript": "^2.7.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1"
  },
  "beemo": {
    "config": "@milesj/build-tool-config",
    "drivers": [
      "babel",
      "eslint",
      "flow",
      "jest",
      "prettier"
    ],
    "execute": {
      "cleanup": false
    },
    "flow": {
      "libs": [
        "./shapeshifter.js.flow",
        "./node_modules/optimal/optimal.js.flow"
      ]
    }
  }
}
