{
  "name": "shapeshifter",
  "version": "5.1.0",
  "description": "Generate relational schemas, React prop types, Flow type aliases, or TypeScript interfaces from JSON, JS, or GraphQL schematic files.",
  "keywords": [
    "generate",
    "schema",
    "schematic",
    "react",
    "prop",
    "flow",
    "typescript",
    "type",
    "alias",
    "definitions",
    "interface",
    "struct",
    "shape",
    "shift",
    "shifter",
    "transpile",
    "entity",
    "graphql"
  ],
  "main": "./lib/index.js",
  "bin": "./bin/shapeshifter.js",
  "scripts": {
    "build": "beemo typescript",
    "check": "type-check && tsc --noEmit --allowJs ./tests/expected/gql/typescript/* ./tests/expected/node/typescript/*",
    "coverage": "yarn run jest --coverage",
    "eslint": "beemo eslint",
    "jest": "beemo jest",
    "posttest": "yarn run eslint --silent",
    "pretest": "yarn run type --silent",
    "preversion": "yarn test",
    "postversion": "yarn run build",
    "test": "yarn run jest --silent",
    "test:webpack": "yarn run babel && webpack --config webpack.config.js",
    "test:webpack:dev": "yarn run babel && webpack-dev-server",
    "flow": "beemo flow check",
    "prettier": "beemo prettier",
    "type": "beemo typescript --noEmit",
    "docs": "gitbook build --log=debug --debug",
    "docs:serve": "gitbook serve",
    "docs:install": "gitbook install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/milesj/shapeshifter.git"
  },
  "author": {
    "name": "Miles Johnson",
    "url": "http://milesj.me"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/milesj/shapeshifter/issues"
  },
  "homepage": "https://github.com/milesj/shapeshifter#readme",
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "graphql": "^0.13.2",
    "optimal": "^0.15.0",
    "vorpal": "^1.12.0"
  },
  "devDependencies": {
    "@milesj/build-tool-config": "^0.75.2",
    "babel-loader": "^7.1.4",
    "flow-bin": "^0.68.0",
    "prop-types": "^15.6.1",
    "typescript": "^2.7.2",
    "webpack": "^4.1.1",
    "webpack-dev-server": "^3.1.1"
  },
  "beemo": {
    "module": "@milesj/build-tool-config",
    "drivers": [
      "babel",
      "eslint",
      "jest",
      "prettier",
      "typescript"
    ],
    "eslint": {
      "ignore": [
        "tests/expected/**/*",
        "tests/bundle.js"
      ]
    }
  }
}
