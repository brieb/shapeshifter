{
  "name": "shapeshifter",
  "version": "5.1.0",
  "description":
    "Generate relational schemas, PropTypes, Flow type aliases, or TypeScript interfaces from JSON, JS, or GraphQL schematic files.",
  "keywords": [
    "generate",
    "schema",
    "schematic",
    "react",
    "prop",
    "prop-types",
    "flow",
    "typescript",
    "type",
    "alias",
    "definitions",
    "interface",
    "struct",
    "shape",
    "shift",
    "shifter",
    "transpile",
    "entity",
    "graphql"
  ],
  "main": "./lib/index.js",
  "module": "./esm/index.js",
  "bin": "./bin/shapeshifter.js",
  "scripts": {
    "babel": "beemo typescript --node",
    "build": "yarn run babel && yarn run build:dts",
    "build:dts": "beemo run-script generate-dts",
    "check": "flow check && tsc --noEmit --lib esnext,esnext.asynciterable ./tests/expected/gql/typescript/*.ts ./tests/expected/node/typescript/*.ts",
    "clean": "rimraf ./{lib,esm}/",
    "coverage": "yarn run jest --coverage",
    "docs": "gitbook build --debug",
    "docs:serve": "gitbook serve",
    "docs:install": "gitbook install",
    "eslint": "beemo eslint",
    "jest": "beemo jest",
    "package": "yarn run clean && yarn run build && yarn test",
    "prettier": "beemo prettier",
    "pretest": "yarn run type",
    "test": "yarn run jest",
    "test:webpack": "yarn run babel && webpack --config webpack.config.js",
    "test:webpack:dev": "yarn run babel && webpack-dev-server",
    "posttest": "yarn run eslint",
    "type": "beemo typescript --noEmit",
    "prerelease": "yarn run package",
    "release": "np --yolo --no-yarn"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/milesj/shapeshifter.git"
  },
  "author": {
    "name": "Miles Johnson",
    "url": "http://milesj.me"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/milesj/shapeshifter/issues"
  },
  "homepage": "https://github.com/milesj/shapeshifter#readme",
  "dependencies": {
    "@types/yargs": "^11.0.0",
    "babel-runtime": "^6.26.0",
    "graphql": "^0.13.2",
    "optimal": "^0.18.0",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "@milesj/build-tool-config": "^0.79.0",
    "@types/graphql": "^0.12.7",
    "flow-bin": "^0.69.0",
    "prop-types": "^15.6.1",
    "typescript": "^2.8.1",
    "webpack": "^4.3.0",
    "webpack-dev-server": "^3.1.1"
  },
  "beemo": {
    "module": "@milesj/build-tool-config",
    "drivers": ["babel", "eslint", "jest", "prettier", "typescript"],
    "eslint": {
      "ignore": ["tests/expected/**/*", "tests/bundle.js"],
      "rules": {
        "function-paren-newline": "off"
      }
    },
    "prettier": {
      "ignore": ["package.json", "tests/expected/**/*"]
    }
  }
}
